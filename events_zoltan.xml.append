<event name="START_BEACON_ZOLTAN">
	<!-- <text>You totally travel through hyperspace lol</text> -->
	<text load="HYPERSPACE_TEXT"/>
	<img back="BG_DARK" planet="PLANET_HYPERSPACE"/>
